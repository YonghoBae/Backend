

            <!--우측 상단 헤더 영역 -->
            <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2>채팅정보관리</h2>
                    <ol class="breadcrumb">
                        <li>
                            <a href="/Home/Index">Home</a>>
                        </li>
                        <li>
                            <a>채팅정보관리</a>>
                        </li>
                        <li class="active">
                            <strong>채팅목록조회</strong>
                        </li>
                    </ol>
                </div>
                <div class="col-lg-2">

                </div>
            </div>

            <!--우측 콘텐츠 영역 -->
            <div class="wrapper wrapper-content">

                <div class="ibox-content m-b-sm border-bottom">
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="form-group">
                                <label class="control-label" for="OrderNo">글제목</label>
                                <input type="text" id="title" class="form-control">
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                <label class="control-label" for="OrderName">아이피주소</label>
                                <input type="text" id="ipaddress" class="form-control">
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                <label class="control-label" for="MobileNo">게시여부</label>
                                <select id="display" name="name" class="form-control">
                                    <option value=9 selected>전체</option>
                                    <option value=1>게시함</option>
                                    <option value=2>게시안함</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="text-center">
                        <button type="button" id="btnSearch" class="btn btn-primary">조회</button>
                    </div>
                </div>

                <div class="row">
                    <div class="col-lg-12">
                        <div class="ibox float-e-margins">
                            <div class="ibox-content">
                                <div class="table-responsive">

                                    <table class="table table-hover">
                                        <thead>
                                            <tr>
                                                <th width="10%">채팅번호</th>
                                                <th width="50%">제목</th>
                                                <th width="5%">아이피주소</th>
                                                <th width="5%">조회수</th>
                                                <th width="10%">게시여부</th>
                                                <th width="10%">등록자번호</th>
                                                <th width="10%">등록일시</th>
                                            </tr>
                                        </thead>
                                        <tbody class="hoverTblBody" id="list">

                                        <% for(var i =0;i<messages.length;i++){ %>

                                            <tr>
                                                    <td><%=messages[i].message_id%></td>
                                                    <td><%=messages[i].title%></td>
                                                    <td><%=messages[i].ip_address%></td>
                                                    <td><%=messages[i].view_cnt%></td>
                                                    <td><%=messages[i].display%></td>
                                                    <td><%=messages[i].regist_id%></td>
                                                    <td><%=messages[i].regist_date%></td>
                                            </tr>

                                        <% } %>
                                           

                           

                                        </tbody>
                                    </table>

                                </div>
                            </div>
                        </div>
                    </div>

                </div>


            </div>